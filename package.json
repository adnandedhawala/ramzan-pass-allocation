    "mongoose": "^7.0.0",
    "next": "13.2.3",
    "next-connect": "^0.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
